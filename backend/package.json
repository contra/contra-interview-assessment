{
  "name": "contra-interview-assessment",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --respawn ./src/bin/server.ts",
    "format-graphql-schema": "format-graphql --write true ./src/schema/schema.graphql",
    "build": "tsc --project ./",
    "lint": "eslint --fix src . && prettier --write src . && tsc --noEmit && format-graphql --write true src/schema/schema.graphql",
    "generate-types": "graphql-codegen",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.21.0",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "1.21.2",
    "@graphql-codegen/core": "1.17.9",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-resolvers": "1.19.0",
    "@jest/globals": "^29.5.0",
    "@slonik/migrator": "0.11.2",
    "@types/jest": "^29.5.0",
    "@types/node": "16.11.*",
    "@types/sequelize": "4.28.14",
    "@typescript-eslint/parser": "5.12.0",
    "eslint": "7.22.0",
    "eslint-config-canonical": "25.9.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "format-graphql": "1.4.0",
    "lint-staged": "10.5.4",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "1.1.6",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "apollo-server-fastify": "3.12.0",
    "fast-safe-stringify": "^2.1.1",
    "fastify": "^3.14.0",
    "fastify-cookie": "^5.4.0",
    "got": "^11.8.5",
    "graphql": "^15.4.0",
    "graphql-import": "^1.0.2",
    "graphql-tools": "^7.0.2",
    "jest": "^29.5.0",
    "pg": "^8.10.0",
    "sequelize": "6.21.4",
    "ts-node": "^9.1.1",
    "winston": "^3.8.2"
  }
}
