{
  "name": "contra-interview-assessment",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development ROARR_LOG=true ts-node-dev --respawn ./src/bin/server.ts | roarr pretty-print",
    "format-graphql-schema": "format-graphql --write true ./src/schema/schema.graphql",
    "build": "tsc --project ./",
    "lint": "eslint --fix src . && prettier --write src . && tsc --noEmit && format-graphql --write true src/schema/schema.graphql",
    "generate-types": "graphql-codegen",
    "migrate:up": "knex migrate:latest --knexfile src/bin/knexfile",
    "migrate:down": "knex migrate:down --knexfile src/bin/knexfile",
    "migrate:create": "knex migrate:make --knexfile src/bin/knexfile",
    "migrate:reset": "knex migrate:rollback --all --knexfile src/bin/knexfile",
    "seed:create": "knex seed:make --knexfile src/bin/knexfile",
    "seed:run": "knex seed:run --knexfile src/bin/knexfile"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "1.21.2",
    "@graphql-codegen/core": "1.17.9",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-resolvers": "1.19.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/parser": "5.12.0",
    "eslint": "7.22.0",
    "eslint-config-canonical": "25.9.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "format-graphql": "1.4.0",
    "lint-staged": "10.5.4",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "ts-node-dev": "1.1.6",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@roarr/cli": "^3.2.2",
    "apollo-server-fastify": "3.6.3",
    "fast-safe-stringify": "^2.1.1",
    "fastify": "^3.14.0",
    "fastify-cookie": "^5.4.0",
    "got": "^11.8.5",
    "graphql": "^15.4.0",
    "graphql-import": "^1.0.2",
    "graphql-tools": "^7.0.2",
    "knex": "^2.4.2",
    "objection": "^3.0.1",
    "pg": "^8.10.0",
    "roarr": "^3.2.0",
    "ts-node": "^9.1.1"
  }
}
