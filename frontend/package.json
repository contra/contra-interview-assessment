    "classnames": "^2.3.1",
    "framer-motion": "^7.2.0",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/jest": "28.1.3",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.8",
    "eslint": "8.18.0",
    "eslint-config-canonical": "35.0.1",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "13.0.3",
